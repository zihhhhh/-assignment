#include <stdio.h>

int main() {
    int n = 5;
 
    int B[5][5];
    int count = 1;
    int R = 0, C = 0;

    B[R][C] = count++;

    while (count <= n * n) {
        if (R + 1 < n) {
            R++;
        }
        else {
            C++;
        }

        B[R][C] = count++;

        while (R - 1 > -1 && C + 1 < n) {
            B[--R][++C] = count++;
        }

        if (C + 1 < n) {
            C++;
        }
        else {
            R++;
        }

        B[R][C] = count++;

        while (C - 1 > -1 && R + 1 < n) {
            B[++R][--C] = count++;
        }
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            printf("%d ", B[i][j]);
        }
        printf("\n");
    }

    return 0;
}
