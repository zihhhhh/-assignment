#include <stdio.h>

int main()
{
	int A[5][5] = {0};
	int cnt = 0, v = 1;
	int n=5;


	while (1) {
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				if (i + j == cnt) {
					A[j][i] = v++;
				}
			}
		}
		cnt++;
		if (v - 1 == n * n) break;
	}

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			printf("%2d ", A[i][j]);
		}
		printf("\n");
	}

	return 0;
}
